{
  "name": "codedetailsweb",
  "version": "0.1.0",
  "private": true,
 "scripts": {
    "validate-env": "node -r ts-node/register ./scripts/validateEnv.ts",
    "set-env": "node -r ts-node/register ./scripts/setEnv.ts",
    "dev": "cross-env NODE_ENV=development npm run set-env && npm run validate-env && next dev",
    "test": "cross-env NODE_ENV=test npm run set-env && npm run validate-env && jest && playwright test",
    "build": "cross-env NODE_ENV=production npm run set-env && npm run validate-env && next build",
    "start": "cross-env NODE_ENV=production npm run set-env && npm run validate-env && next start"
  },
  "dependencies": {
    "@algolia/client-search": "^5.20.3",
    "@clerk/nextjs": "^6.12.1",
    "@supabase/supabase-js": "^2.49.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.4.7",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "postcss-load-config": "^6.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "shadcn-ui": "^0.9.5",
    "stripe": "^17.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/postcss-load-config": "^3.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "supabase": "^2.15.8",
    "tailwindcss": "^4.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
